<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <display-name>BookBook</display-name>

    <welcome-file-list>
        <welcome-file>jsp/homepage.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>bookbook</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>bookbook</servlet-name>
        <url-pattern>*.page</url-pattern>
    </servlet-mapping>
</web-app>